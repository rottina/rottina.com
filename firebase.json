{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  },
  "redirects": [
    {
      "source": "/wiffleball/**",
      "destination": "https://web.archive.org/web/20021022190712/http://rottina.com/wiffleball/",
      "type": 302
    }
  ],
  "storage": {
    "rules": "storage.rules"
  },
  "remoteconfig": {
    "template": "remoteconfig.template.json"
  }
}


